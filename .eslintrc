{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "describe": true,
        "it": true
    },
    "extends": ["airbnb"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "indent": [2, 4],
        "no-underscore-dangle": [2, { "allowAfterThis": true }]
    }
}